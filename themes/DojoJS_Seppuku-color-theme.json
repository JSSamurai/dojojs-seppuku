        {
            "name": "PARAMETER",
            "scope": ["variable.parameter", "meta.parameter", "meta.function.parameters"],
            "settings": {
                "foreground": "#58B8E8"
            }
        },

        {
            "name": "METHOD",
            "scope": ["entity.name.function.method", "entity.name.function.accessor"],
            "settings": {
                "foreground": "#FF6468"
            }
        },

        {
            "name": "JSDoc Classes",
            "scope": ["storage.type.class.jsdoc"],
            "settings": {
                "foreground": "#B05090",
                "fontStyle": ""
            }
