            "settings": {
                "foreground": "#40A000",
                "fontStyle": "italic"
            }
        },

        {
            //  ->>  L3
            "name": "Method-Declaration-Names",
            "scope": "meta.definition.method",
            "settings": {
                "foreground": "#1BBB7B",
                "fontStyle": ""
            }
