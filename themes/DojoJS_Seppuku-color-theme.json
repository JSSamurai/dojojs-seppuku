            "name": "STORAGE",
            "scope": ["storage"],
            "settings": {
                "foreground": "#ff802c",
                "fontStyle": ""
            }
        },

        {
            "name": "KEYWORD -",
            "scope": ["keyword.operator"],
            "settings": {
                "foreground": "#ff802c",
                "fontStyle": ""
            }
        },

        {
            "name": "KEYWORD -",
            "scope": [
                "keyword.operator.logical",
                "keyword.operator.arithmetic",
                "keyword.operator.ternary"
            ],

            "settings": {
                "foreground": "#ff802c",
                "fontStyle": "bold"
            }
        },

        {
            "name": "KEYWORD -",
            "scope": ["keyword.operator.private.js"],
            "settings": {
                "fontStyle": "",
                "foreground": "#ff802c"
            }
        },

        {
            "name": "KEYWORD |> OPERATOR |> (ARITHMETIC, LOGICAL, & TERNARY)",
            "scope": [
                "keyword.operator.logical",
                "keyword.operator.arithmetic",
                "keyword.operator.ternary"
            ],
            "settings": {
                "foreground": "#ff7010",
                "fontStyle": "bold"
            }
        },

        {
            "name": "KEYWORD",
            "scope": ["keyword.operator.expression", "keyword.control.flow"],
            "settings": {
                "fontStyle": "italic",
                "foreground": "#ff7014"
            }
        },
        {
            "name": "KEYWORD |> OPERATOR |> DECREMENT",
            "scope": ["keyword.operator.increment"],
            "settings": {
                "foreground": "#249F30",
                "fontStyle": "bold"
            }
        },

        {
            "name": "KEYWORD |> OPERATOR |> DECREMENT",
            "scope": ["keyword.operator.decrement"],
            "settings": {
                "foreground": "#CF2424",
                "fontStyle": "bold"
            }
        },

        {
            "name": "OBJECTS/CLASS-OBJECTS",
            "scope": ["variable.other.object", "support.class.node"],
            "settings": {
                "foreground": "#ee6065",
                "fontStyle": ""
            }
