                base: "#70B8E8", ("source.js"/"source.json")
                alpha: "",
                beta: "",
                delta: "",
                gamma: "",
                
            }
        */

        {
            "name": "JavaScript",
            "scope": "source.js",
            "settings": {
                "foreground": "#70B8E8",
                "fontStyle": ""
            }
        },

        {
            "name": "JSON",
            "scope": "source.json",
            "settings": {
                "foreground": "#70B8E8",
                "fontStyle": ""
            }
        },

        {
            "name": "Comments (style: italic)",
            "scope": "comment",
            "settings": {
                "foreground": "#3A6A90",
                "fontStyle": "italic"
            }
        },

        {
            "name": "Strings Quoted",
            "scope": ["string.quoted.single", "string.quoted.double"],
            "settings": {
                "foreground": "#B8B48E",
                "fontStyle": ""
            }
        },

        {
            "name": "JSON/JS-Object Properties ",
            "scope": ["support.type.property-name.json", "string.unquoted.js"],
            "settings": {
                "foreground": "#70B8E8",
                "fontStyle": ""
            } // NOTE: "Highlights unquoted JavaScript properties, and quoted JSON properties."
        },

        {
            "name": "Template-Literal (style: italic)",
            "scope": ["string.quoted.template.js", "string.quasi.js"],
            "settings": {
                "foreground": "#70B8E8",
                "fontStyle": "italic"
            }
        },

        {
            "name": "Puncuation (style: default)",
            "scope": [
                "punctuation.support.type.property-name.end.json",
                "punctuation.support.type.property-name.begin.json",
                "punctuation.separator.dictionary.key-value.json",
                "punctuation.definition.dictionary.begin.json",
                "punctuation.definition.dictionary.end.json",
                "punctuation.separator.dictionary.pair.json",
                "punctuation.definition.string.begin.json",
                "punctuation.definition.string.end.json",
                "punctuation.separator.array.json",
                "punctuation.terminator.statement",
                "punctuation.separator.key-value",
                "punctuation.section.class.begin",
                "punctuation.definition.comment",
                "punctuation.section.class.end",
                "meta.delimiter.comma",
                "meta.brace.square",
                "meta.brace.round",
                "meta.brace.curly"
            ],
            "settings": {
                "foreground": "#D8584C",
                "fontStyle": ""
            }
        },

        {
            "name": "Small-Operators (style: bold)",
            "scope": ["keyword.operator.accessor"],
            "settings": {
                "foreground": "#D8584C",
                "fontStyle": "bold"
            }
        },

        {
            "name": "Operators: Arithmitic, Relational, Assignment/Augmented-Assignment",
            // (+), (-), (*), (/), (+=), (-=), (*=), (/=),
            "scope": [
                "keyword.operator.assignment",
                "keyword.operator.relational",
                "keyword.operator.assignment.augmented",
                "keyword.operator.arithmetic"
            ],

            "settings": {
                "foreground": "#70B8E8", // #E86E1E
                "fontStyle": ""
            }
        },

        {
            "name": "Contoling/Dynamic-Keywords, Storage/Static-Keywords",
            "scope": [
                "keyword.control.flow", // (Return, Await)
                "keyword.control.loop",
                "keyword.control.trycatch", // (Try, Catch, Throw, Finally)
                "keyword.control.conditional" // (If, else, if else)
            ],
            "settings": {
                "foreground": "#70B8E8",
                "fontStyle": ""
            }
        },

        {
            "name": "Storage-Types",
            "scope": [
                "storage.type",
                "storage.type.class",
                "storage.type.extends",
                "storage.type.accessor", // (Get, Set)
                "storage.type.function.arrow",
                "keyword.operator.private.js"
            ],
            "settings": {
                "foreground": "#70B8E8", // #E8B440
                "fontStyle": ""
            }
        },

        {
            "name": "Functions & Methods",
            "scope": [
                "entity.name.function",
                "entity.name.function.method",
                "entity.name.class",
                "support.function"
            ],
            "settings": {
                "foreground": "#70B8E8",
                "fontStyle": ""
            }
        },

        {
            "name": "Objects",
            "scope": [
                "variable.other.object",
                "support.class.node",
                "support.type.object.console",
                "support.type.object"
            ],
            "settings": {
                "foreground": "#70B8E8",
                "fontStyle": ""
            }
        },

        {
            "name": "Execution-Context & Parameters",
            "scope": ["meta.function.parameters", "variable.language.this", "variable.parameter"],
            "settings": {
                "foreground": "#70B8E8",
                "fontStyle": "italic"
            }
        },

        {
            "name": "Constants",
            "scope": ["variable.other.constant"],
            "settings": {
                "foreground": "#70B8E8",
                "fontStyle": ""
            }
        },

        /*  ----------------------------------------------------------




                    <==*====================| JSDOCS |====================*==>                     */
        {
            "name": "JSDocs: Puncuation",
            "scope": [
                "punctuation.definition.block.tag.jsdoc",
                "punctuation.definition.bracket.curly.begin.jsdoc",
                "punctuation.definition.bracket.curly.end.jsdoc"
            ],

            "settings": {
                "foreground": "#70B8E8", //#AAAA40
                "fontStyle": ""
            }
        },

        {
            "name": "",
            "scope": "storage.type.class.jsdoc",
            "settings": {
                "foreground": "#70B8E8", // #208028
                "fontStyle": ""
            }
        },

        {
            "name": "JSDocs: Entities",
            "scope": "entity.name.type.instance.jsdoc",
            "settings": {
                "foreground": "#70B8E8", // #AAAA40
                "fontStyle": ""
            }
        },

        {
            "name": "JSDocs: Variable",
            "scope": "variable.other.jsdoc",
            "settings": {
                "foreground": "#70B8E8", // #AAAAAA
                "fontStyle": ""
            }
        }
    ]
}