  },
  "tokenColors": [
    {
      "name": "[L0|Language JavaScript] ",
      "scope": "source.js",
      "settings": {
        "foreground": "#AAAAAF",
        "fontStyle": ""
      }
    },
    {
      "name": "[L0|Language JSON] ",
      "scope": "source.json",
      "settings": {
        "foreground": "#A0A0A8",
        "fontStyle": ""
      }
    },
    {
      "name": "Comment",
      "scope": "comment",
      "settings": {
        "foreground": "#505060",
        "fontStyle": "italic"
      }
    },
    {
      "name": "JSON Property, JS Object Property",
      "scope": ["support.type.property-name.json", "string.unquoted.js"],
      "settings": {
        "foreground": "#989898",
        "fontStyle": "bold"
      }
    },
    {
      "name": "JSON Property's Value",
      "scope": "string.quoted.double.json",
      "settings": {
        "foreground": "#D8646c",
        "fontStyle": ""
      }
    },
    {
      "name": "Meta",
      "scope": "meta",
      "settings": {
        "foreground": "#A0A0A8",
        "fontStyle": ""
      }
    },
    {
      "name": "Punctuation: All",
      "scope": "punctuation",
      "settings": {
        "foreground": "#C8A444",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Puncuation: Braces",
      "scope": ["meta.brace", "punctuation.definition.block"],
      "settings": {
        "foreground": "#C8A444",
        "fontStyle": ""
      }
    },
    {
      //  =>  LvL: S1
      "name": "Storage",
      "scope": "storage",
      "settings": {
        "foreground": "#F06F34",
        "fontStyle": ""
      }
    },
    {
      //  |>  LvL: S2
      "name": "Control Keywords",
      "scope": "keyword.control",
      "settings": {
        "foreground": "#FF5C64",
        "fontStyle": ""
      }
    },
    //  |>  LvL: S2
    {
      "name": "Operators",
      "scope": "keyword.operator",
      "settings": {
        "foreground": "#FF6C20",
        "fontStyle": "bold"
      }
    },
    {
      //  |>  LvL: S2
      "name": "Operators",
      "scope": [
        "keyword.operator.expression",
        "keyword.operator.new",
        "keyword.operator.assignment"
      ],
      "settings": {
        "foreground": "#FF6C20",
        "fontStyle": ""
      }
    },
    {
      //  |>  LvL: S2
      "name": "Parameters",
      "scope": [
        "variable.parameter.js",
        "variable.parameter",
        "meta.parameters",
        "meta.function.parameters"
      ],
      "settings": {
        "foreground": "#1BBBFB",
        "fontStyle": "italic"
      }
    },
    {
      //  ->>  LvL: S3
      "name": "This",
      "scope": "variable.language.this",
      "settings": {
        "foreground": "#0BBBFB",
        "fontStyle": "italic"
      }
    },

    {
      //  =>  LvL: S1
      "name": "String",
      "scope": "string",
      "settings": {
        "foreground": "#D54555",
        "fontStyle": ""
      }
    },

    {
      //  ->>  LvL: S3
      "name": "",
      "scope": "meta.definition.method",
      "settings": {
        "foreground": "#3CC888",
        "fontStyle": "bold"
      }
    },

    {
      "name": "Numbers",
      "scope": ["constant.numeric"],
      "settings": {
        "foreground": "#EE6470",
        "fontStyle": ""
      }
    },

    {
      "name": "Escaped Characters (Example: '\n')",
      "scope": "constant.character.escape",
      "settings": {
        "foreground": "#3482F8",
        "fontStyle": ""
      }
    }

